{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "NylasEnv": false
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "strict": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "react/sort-comp": [2, {
      "order": [
        'displayName',
        'containerStyles',
        'static-methods',
        'lifecycle',
        'everything-else',
        'render'
      ]
    }]
  }
}
